<%= form_for @festival do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :location %>
<%= f.text_field :location %>
<br>
<%= f.label :start_date %>
<%= f.text_field :start_date %>
<br>
<%= f.label :end_date %>
<%= f.text_field :end_date %>
<br>
<%= f.label :image_url %>
<%= f.text_field :image_url %>
<%= f.submit %>
<% end %>
